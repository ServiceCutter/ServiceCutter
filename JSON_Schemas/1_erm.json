{
   "$schema":"http://json-schema.org/draft-03/schema#",
   "type":"object",
   "additionalProperties": false,
   "properties":{
      "name":{
         "type":"string"
      },
      "entities":{
         "type":"array",
         "items":{
            "type":"object",
            "additionalProperties": false,
            "properties":{
               "name":{
                  "type":"string"
               },
               "nanoentities":{
                  "type":"array",
                  "items":{
                     "type":"string"
                  }
               }
            }
         }
      },
      "relations":{
         "type":"array",
         "items":{
            "type":"object",
            "additionalProperties": false,
            "properties":{
               "type":{
                  "type":"string",
                  "enum":[
                     "AGGREGATION",
                     "COMPOSITION",
                     "INHERITANCE"
                  ]
               },
               "destination":{
                  "type":"string"
               },
               "origin":{
                  "type":"string"
               }
            }
         }
      }
   }
}